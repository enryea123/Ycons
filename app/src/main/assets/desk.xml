                    Image="" Margins="" />
                <Wallpaper Border="1" BorderColor="" BorderLine="" Color="" Fill="1" Identity=""
                    Image="" Margins="" />
            </ShowItemLayer>
        </IconStyle>
    </CommonStyles>

    <Screen Source="screen.xml">
        <ScreenStyles>
            <ScreenIconStyle Halign="" Height="" Margins="{1,1,0,0}" ShowLightMode="1" TextHalign=""
                TextValign="" Valign="" Width="">
                <Wallpaper Border="1" BorderColor="" BorderLine="" Color="" Fill="1"
                    Identity="Text_bg" Image="application_background" Margins="" />
            </ScreenIconStyle>
            <FolderStyle>
                <Wallpaper Border="1" BorderColor="" BorderLine="" Color="" Fill="1" Identity=""
                    Image="appfunc_folderback" Margins="" />
                <Wallpaper Border="1" BorderColor="" BorderLine="" Color="" Fill="1"
