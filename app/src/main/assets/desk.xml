            </IndicatorItem>
            <IndicatorItem Height="" Identity="Slide" Margins="{1,1,0,0}" Width="">
                <Wallpaper Border="1" BorderColor="" BorderLine="" Color="" Fill="1"
                    Identity="Selected" Image="screennow" Margins="" />
            </IndicatorItem>
        </IndicatorStyles>
    </Indicator>

