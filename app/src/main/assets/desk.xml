                <Wallpaper Border="1" BorderColor="" BorderLine="" Color="" Fill="1"
                    Identity="ColseFolder" Image="appfunc_folder_close_up" Margins="" />
            </FolderStyle>
            <Light Color="" Image="" Size="" />
            <TrashStyle Color="">
                <Trashing Halign="" Image="d_del" Margins="{1,1,0,0}" Valign="">
                    <Wallpaper Border="1" BorderColor="" BorderLine="" Color="" Fill="1" Identity=""
                        Image="trash_bg_normal" Margins="" />
                </Trashing>
                <Trashed Halign="" Image="del" Margins="{1,1,0,0}" Valign="">
                    <Wallpaper Border="1" BorderColor="" BorderLine="" Color="" Fill="1" Identity=""
                        Image="trash_bg_hover" Margins="" />
                </Trashed>
            </TrashStyle>
            <Font Color="" Size="" />
        </ScreenStyles>
    </Screen>

    <Indicator Source="indicator.xml">
        <IndicatorStyles ShowMode="1" WhenScreenCount="">
            <IndicatorItem Height="24" Identity="Dots" Margins="{1,1,0,0}" Width="24">
                <Wallpaper Border="1" BorderColor="" BorderLine="" Color="" Fill="1"
                    Identity="Selected" Image="screennow" Margins="" />
